{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "webpack serve --open --config ./webpack.development.js",
    "start:prod": "webpack --config ./webpack.production.js",
    "build": "webpack --config webpack.base.js",
    "profile": "webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "prettier": "prettier --ignore-path .prettierignore --write \"**/*.+(json|js|ts|tsx)\"",
    "lint": "eslint src --fix"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module-and-prefix",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module-and-prefix",
      "parser": "babylon"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/register": "^7.12.10",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-loader": "^1.3.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "use-react-router": "^1.0.7",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@svgr/webpack": "^5.4.0",
    "@teamsupercell/typings-for-css-modules-loader": "teamsupercell/typings-for-css-modules-loader",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.0.2",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "core-js": "^3.7.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "fork-ts-checker-webpack-plugin": "6.0.7",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "0.12.3",
    "import-sort-style-module-and-prefix": "^0.1.3",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.3",
    "path": "^0.12.7",
    "postcss-loader": "^4.0.4",
    "pretty-quick": "^3.1.0",
    "react-hot-loader": "^4.5.3",
    "react-refresh": "^0.9.0",
    "sass": "^1.32.2",
    "sass-loader": "^10.1.0",
    "sass-resources-loader": "^2.1.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3"
  }
}
